{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"experimentalSortImports": {
		"groups": ["type-import", ["value-builtin", "value-external"], "type-internal", "value-internal"],
		"newlinesBetween": true,
		"ignoreCase": true
	}
}
