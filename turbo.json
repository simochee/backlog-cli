{
	"$schema": "node_modules/turbo/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "out/**", "tsp-output/**", "src/generated/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"test": {},
		"type-check": {
			"dependsOn": ["^build", "^type-check"]
		}
	}
}
